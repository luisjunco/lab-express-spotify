<h1>Tracklist</h1>

<div class="item-list">
    {{#each tracks}}
        <div class="item track">
            <div class="track-title">
                {{this.name}}
            </div>
            <div class="track-audio">
                <audio controls src="{{this.preview_url}}"></audio>
            </div>
        </div>
    {{/each}}
</div>